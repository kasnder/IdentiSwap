{
  "manifest_version": 2,
  "name": "IdentiSwap",
  "version": "1.0.10",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "icons/swap-48.png",
    "96": "icons/swap-96.png",
    "128": "icons/swap-128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "loader.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "main.js"
  ]
}
