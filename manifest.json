{
  "manifest_version": 3,
  "name": "IdentiSwap",
  "version": "1.1.0",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "icons/swap-48.png",
    "96": "icons/swap-96.png",
    "128": "icons/swap-128.png"
  },
  "permissions": ["storage"],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "loader.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
